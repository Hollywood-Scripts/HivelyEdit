<?xml version="1.0" encoding="iso-8859-1"?>
<dialog id="tracked" accelerator="acc_track" title="Note Edit">
	<vgroup>
		<hgroup>
			<vgroup frame="true">
				<label align="center">Note</label>
				<choice id="trknote">
					<item>None</item>
					<item>C</item>
					<item>C#</item>
					<item>D</item>
					<item>D#</item>
					<item>E</item>
					<item>F</item>
					<item>F#</item>
					<item>G</item>
					<item>G#</item>
					<item>A</item>
					<item>A#</item>
					<item>B</item>
				</choice>
				<label align="center">Octave</label>
				<choice id="trkoct">
					<item>0</item>
					<item>1</item>
					<item>2</item>
					<item>3</item>
					<item>4</item>
					<item>5</item>
				</choice>
			</vgroup>
			<rectangle />
			<vgroup frame="true">
				<label align="center">Instruemnt</label>
				<slider max="63" horiz="false" />
			</vgroup>
			<vgroup frame="true">
				<label align="center">Command 1</label>
				<choice id="cmd1">
					<item>None</item>
					<item>Portamento Up</item>
					<item>Portamento Down</item>
					<item>Tone Portamento</item>
					<item>Set Filter</item>
					<item>Tone Portamento//Volume Slide</item>
					<item>Panning</item>
					<item>Pulse Width</item>
					<item>Volume Slide</item>
					<item>Jump</item>
					<item>Volume</item>
					<item>Next Block</item>
					<item>Tempo</item>
					<item>Long Jump</item>
					<item>Pitch Slide Up</item>
					<item>Pitch Slide Down</item>
					<item>Vibrato Depth</item>
					<item>Fine Volume Up</item>
					<item>Fine Volume Down</item>
					<item>Note Cut</item>
					<item>Note Delay</item>
					<item>Flags</item>
				</choice>

				<pageview mode="none">
					<vgroup title="None">
						<rectangle />
					</vgroup>

					<vgroup title="Portamento Up">
						<slider id="trkporup1" />
					</vgroup>
					
					<vgroup title="Portamento Down">
						<slider id="trkpordown1" />
					</vgroup>
					
					<vgroup title="Tone Portamento">
						<slider id="trkpor1" />
					</vgroup>
					
					<vgroup title="Set Filter">
						<choice id="trkfiltmode1">
							<item>Off</item>
							<item>Low Pass</item>
							<item>High Pass</item>
						</choice>
						<slider id="trkfltres1" min="1" max="31" />				
					</vgroup>
					
					<vgroup title="Tone Portamento//Volume Slide">
						<slider id="trkporvol1" />
						<slider id="trkvolpor1" />
					</vgroup>
					
					<vgroup title="Panning">
						<slider id="trkpan1" min="-128" max="127" />
					</vgroup>
					
					<vgroup title="Pulse Width">
						<slider id="trkpw1" />
					</vgroup>
					
					<vgroup title="Volume Slide">
						<choice id="trkvolslide1">
							<item>Up</item>
							<item>Down</item>
						</choice>
						<slider id="trkvolsliderate1" min="0" max="15" />
					</vgroup>
					
					<vgroup title="Jump">
						<slider id="trkjmp1" />
					</vgroup>

					<vgroup title="Volume">
						<choice id="trkvolmode1">
							<item>Track</item>
							<item>Instrument</item>
							<item>Master</item>
						</choice>
						<slider id="trkvol1" min="0" max="64" />
					</vgroup>
					
					<vgroup title="Next Block">
						<slider id="trkblk1" min="0" max="255" />
					</vgroup>
					
					<vgroup title="Tempo">
						<slider id="trktempo1" />
					</vgroup>
								
					<vgroup title="Long Jump">
						<slider id="trklongjmp1" />
					</vgroup>
					
					<vgroup title="Pitch Slide Up">
						<slider id="trkpitchup1" max="15" />
					</vgroup>
					
					<vgroup title="Pitch Slide Down">
						<slider id="trkpitchdown1" max="15" />
					</vgroup>
					
					<vgroup title="Vibrato Depth">
						<slider id="trkvibdep1" max="15" />
					</vgroup>
					
					<vgroup title="Fine Volume Up">
						<slider id="trkvolup1" max="15" />
					</vgroup>
					
					<vgroup title="Fine Volume Down">
						<slider id="trkvoldown1" max="15" />
					</vgroup>
					
					<vgroup title="Note Cut">
						<slider id="trknotecut1" max="15" />
					</vgroup>
					
					<vgroup title="Note Delay">
						<slider id="trknotedelay1" max="15" />
					</vgroup>
					
					<vgroup title="Flags">
						<checkbox right="true" selected="true" id="trktrnsfix1">Transpose Hold</checkbox>
					</vgroup>
					
				</pageview>
				<rectangle />
			</vgroup>
			<vgroup frame="true">
				<label align="center">Command 2</label>
				<choice id="cmd2">
					<item>None</item>
					<item>Portamento Up</item>
					<item>Portamento Down</item>
					<item>Tone Portamento</item>
					<item>Set Filter</item>
					<item>Tone Portamento//Volume Slide</item>
					<item>Panning</item>
					<item>Pulse Width</item>
					<item>Volume Slide</item>
					<item>Jump</item>
					<item>Volume</item>
					<item>Next Block</item>
					<item>Tempo</item>
					<item>Long Jump</item>
					<item>Pitch Slide Up</item>
					<item>Pitch Slide Down</item>
					<item>Vibrato Depth</item>
					<item>Fine Volume Up</item>
					<item>Fine Volume Down</item>
					<item>Note Cut</item>
					<item>Note Delay</item>
					<item>Flags</item>
				</choice>

				<pageview mode="none">
					<vgroup title="None">
						<rectangle />
					</vgroup>

					<vgroup title="Portamento Up">
						<slider id="trkporup2" />
					</vgroup>
					
					<vgroup title="Portamento Down">
						<slider id="trkpordown2" />
					</vgroup>
					
					<vgroup title="Tone Portamento">
						<slider id="trkpor2" />
					</vgroup>
					
					<vgroup title="Set Filter">
						<choice id="trkfiltmode2">
							<item>Off</item>
							<item>Low Pass</item>
							<item>High Pass</item>
						</choice>
						<slider id="trkfltres2" min="1" max="31" />				
					</vgroup>
					
					<vgroup title="Tone Portamento//Volume Slide">
						<slider id="trkporvol2" />
						<slider id="trkvolpor2" />
					</vgroup>
					
					<vgroup title="Panning">
						<slider id="trkpan2" min="-128" max="127" />
					</vgroup>
					
					<vgroup title="Pulse Width">
						<slider id="trkpw2" />
					</vgroup>
					
					<vgroup title="Volume Slide">
						<choice id="trkvolslide2">
							<item>Up</item>
							<item>Down</item>
						</choice>
						<slider id="trkvolsliderate2" min="0" max="15" />					
					</vgroup>
					
					<vgroup title="Jump">
						<slider id="trkjmp2" />
					</vgroup>

					<vgroup title="Volume">
						<choice id="trkvolmode2">
							<item>Track</item>
							<item>Instrument</item>
							<item>Master</item>
						</choice>
						<slider id="trkvol2" min="0" max="64" />
					</vgroup>
					
					<vgroup title="Next Block">
						<slider id="trkblk2" min="0" max="255" />
					</vgroup>
					
					<vgroup title="Tempo">
						<slider id="trktempo2" />
					</vgroup>
								
					<vgroup title="Long Jump">
						<slider id="trklongjmp2" />
					</vgroup>
					
					<vgroup title="Pitch Slide Up">
						<slider id="trkpitchup2" max="15" />
					</vgroup>
					
					<vgroup title="Pitch Slide Down">
						<slider id="trkpitchdown2" max="15" />
					</vgroup>
					
					<vgroup title="Vibrato Depth">
						<slider id="trkvibdep2" max="15" />
					</vgroup>
					
					<vgroup title="Fine Volume Up">
						<slider id="trkvolup2" max="15" />
					</vgroup>
					
					<vgroup title="Fine Volume Down">
						<slider id="trkvoldown2" max="15" />
					</vgroup>
					
					<vgroup title="Note Cut">
						<slider id="trknotecut2" max="15" />
					</vgroup>
					
					<vgroup title="Note Delay">
						<slider id="trknotedelay2" max="15" />
					</vgroup>
					
					<vgroup title="Flags">
						<checkbox right="true" selected="true" id="trktrnsfix2">Transpose Hold</checkbox>
					</vgroup>
					
				</pageview>
				<rectangle />
			</vgroup>
		</hgroup>
		<hgroup>
			<button id="trkok">OK</button>
			<rectangle />
			<button id="trkcancel">Cancel</button>
		</hgroup>
	</vgroup>
</dialog>
