<?xml version="1.0" encoding="iso-8859-1"?>
<dialog id="insted">
	<vgroup>
		<hgroup>
			<vgroup frame="true">
				<label align="center">Transpose</label>
				<hline />
				<colgroup columns="2">
					<label>Note</label>
					<choice id="instnote">
						<item>A</item>
						<item>A#</item>
						<item>B</item>
						<item>C</item>
						<item>C#</item>
						<item>D</item>
						<item>D#</item>
						<item>E</item>
						<item>F</item>
						<item>F#</item>
						<item>G</item>
						<item>G#</item>
					</choice>
					<label>Octave</label>
					<choice id="instoct">
						<item>0</item>
						<item>1</item>
						<item>2</item>
						<item>3</item>
						<item>4</item>
						<item>5</item>
					</choice>
					<label>Waveform</label>
					<choice id="instwave">
						<item>None</item>
						<item>Noise</item>
						<item>Pulse</item>
						<item>Sawtooth</item>
						<item>Triangle</item>
					</choice>
				</colgroup>
				<rectangle />
			</vgroup>
			<vgroup frame="true">
				<label align="center">Command 1</label>
				<choice id="instcmd1">
					<item>None</item>
					<item>Set Filter</item>
					<item>Portamento Up</item>
					<item>Portamento Down</item>
					<item>Pulse Width</item>
					<item>Pulse and Filter Modulation</item>
					<item>Jump</item>
					<item>Ring Modulation</item>
					<item>Panning</item>
					<item>Volume</item>
					<item>List Speed</item>
				</choice>
				<pageview mode="none">

					<vgroup title="None">
						<rectangle />
					</vgroup>

					<vgroup title="Set Filter">
						<choice id="instfiltmode1">
							<item>Off</item>
							<item>Low Pass</item>
							<item>High Pass</item>
						</choice>
						<slider id="fltres1" min="1" max="31" />
					</vgroup>

					<vgroup title="Portamento Up">
						<slider id="slduprate1" />
					</vgroup>

					<vgroup title="Portamento Down">
						<slider id="slddownrate1" />
					</vgroup>

					<vgroup title="Pulse Width">
						<slider id="pw1" />
					</vgroup>

					<vgroup title="Pulse and Filter Modulation">
						<label>Filter Modulation</label>
						<choice id="filtmodmode1">
							<item>Stop</item>
							<item>Foreward</item>
							<item>Reverse</item>
						</choice>
						<hline />
						<label>Pulse Width Modulation</label>
						<choice id="pwmodmode1">
							<item>Stop</item>
							<item>Foreward</item>
							<item>Reverse</item>
						</choice>
					</vgroup>
					
					<vgroup title="Jump">
						<slider id="jmp1" />
					</vgroup>

					<vgroup title="Ring Modulation">
						<choice id="instringtype1">
							<item>Triangle Absolute</item>
							<item>Sawtooth Absolute</item>
							<item>Triangle Relative</item>
							<item>Sawtooth Relative</item>
							<item>Triangle Stop</item>
							<item>Sawtooth Stop</item>
						</choice>
						<label>Note</label>
						<choice id="rmnote1">
							<item>A</item>
							<item>A#</item>
							<item>B</item>
							<item>C</item>
							<item>C#</item>
							<item>D</item>
							<item>D#</item>
							<item>E</item>
							<item>F</item>
							<item>F#</item>
							<item>G</item>
							<item>G#</item>
						</choice>
						<label>Octave</label>
						<choice id="rmoct1">
							<item>0</item>
							<item>1</item>
							<item>2</item>
							<item>3</item>
							<item>4</item>
							<item>5</item>
						</choice>
					</vgroup>

					<vgroup title="Panning">
						<slider id="instpan1" min="-128" max="127" />
					</vgroup>

					<vgroup title="Volume">
						<choice id="instvolmode1">
							<item>Track</item>
							<item>Instrument</item>
							<item>Master</item>
						</choice>
						<slider id="instvol1" min="0" max="64" />
					</vgroup>

					<vgroup title="List Speed">
						<slider id="insttempo1" min="0" max="255" />
					</vgroup>

				</pageview>
			</vgroup>
			<vgroup frame="true">
				<label align="center">Command 2</label>
				<choice id="instcmd2">
					<item>None</item>
					<item>Set Filter</item>
					<item>Portamento Up</item>
					<item>Portamento Down</item>
					<item>Pulse Width</item>
					<item>Pulse and Filter Modulation</item>
					<item>Jump</item>
					<item>Ring Modulation</item>
					<item>Panning</item>
					<item>Volume</item>
					<item>List Speed</item>
				</choice>

				<pageview mode="none">

					<vgroup title="None">
						<rectangle />
					</vgroup>

					<vgroup title="Set Filter">
						<choice id="instfiltmode2">
							<item>Off</item>
							<item>Low Pass</item>
							<item>High Pass</item>
						</choice>
						<slider id="fltres2" min="1" max="31" />
					</vgroup>

					<vgroup title="Portamento Up">
						<slider id="slduprate2" />
					</vgroup>

					<vgroup title="Portamento Down">
						<slider id="slddownrate2" />
					</vgroup>

					<vgroup title="Pulse Width">
						<slider id="pw2" />
					</vgroup>

					<vgroup title="Pulse and Filter Modulation">
						<label>Filter Modulation</label>
						<choice id="filtmodmode2">
							<item>Stop</item>
							<item>Foreward</item>
							<item>Reverse</item>
						</choice>
						<hline />
						<label>Pulse Width Modulation</label>
						<choice id="pwmodmode2">
							<item>Stop</item>
							<item>Foreward</item>
							<item>Reverse</item>
						</choice>
					</vgroup>
					
					<vgroup title="Jump">
						<slider id="jmp2" />
					</vgroup>

					<vgroup title="Ring Modulation">
						<choice id="instringtype2">
							<item>Triangle Absolute</item>
							<item>Sawtooth Absolute</item>
							<item>Triangle Relative</item>
							<item>Sawtooth Relative</item>
							<item>Triangle Stop</item>
							<item>Sawtooth Stop</item>
						</choice>
						<label>Note</label>
						<choice id="rmnote2">
							<item>A</item>
							<item>A#</item>
							<item>B</item>
							<item>C</item>
							<item>C#</item>
							<item>D</item>
							<item>D#</item>
							<item>E</item>
							<item>F</item>
							<item>F#</item>
							<item>G</item>
							<item>G#</item>
						</choice>
						<label>Octave</label>
						<choice id="rmoct2">
							<item>0</item>
							<item>1</item>
							<item>2</item>
							<item>3</item>
							<item>4</item>
							<item>5</item>
						</choice>
					</vgroup>

					<vgroup title="Panning">
						<slider id="instpan2" min="-128" max="127" />
					</vgroup>

					<vgroup title="Volume">
						<choice id="instvolmode2">
							<item>Track</item>
							<item>Instrument</item>
							<item>Master</item>
						</choice>
						<slider id="instvol2" min="0" max="64" />
					</vgroup>

					<vgroup title="List Speed">
						<slider id="insttempo2" min="0" max="255" />
					</vgroup>

				</pageview>
			</vgroup>
		</hgroup>
		<rectangle />
		<hgroup>
			<button id="instok">OK</button>
			<button id="instcancel">Cancel</button>
		</hgroup>
	</vgroup>
</dialog>